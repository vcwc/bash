#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export VCW_STACK_GLOBAL_STACKS="vcwc/bash/wrapper"
export VCW_STACK_GLOBAL_STACKNAMES="vcw_stack_bash_wrapper"
export VCW_STACK_GLOBAL_BOOTSTACKS=""
export VCW_STACK_GLOBAL_RUNSTACKS="vcw_stack_bash_wrapper"
export VCW_STACK_GLOBAL_VOLUME_LIST=""
# END CONFIGURATION

# START GLOBAL SETTINGS
export VCW_STACK_GLOBAL='\
$VCW_STACK_GLOBAL_SERVICE_NAME,\
$VCW_STACK_GLOBAL_BASH_WRAPPER_NAME,\
$VCW_STACK_BASH_WRAPPER_ENVIRONMENT,\
$VCW_STACK_BASH_WRAPPER_COMMAND,\
$VCW_STACK_BASH_WRAPPER_VOLUMES,\
$VCW_STACK_BASH_WRAPPER_NETWORKS,\
$VCW_STACK_BASH_WRAPPER_PORTS,\
$VCW_STACK_BASH_WRAPPER_DEPLOY_MODE,\
$VCW_STACK_BASH_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY,\
$VCW_STACK_BASH_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY'
export VCW_STACK_GLOBAL_SERVICE_NAME="bash_wrapper"
export VCW_STACK_GLOBAL_IMAGE_NAME="vcwc/bash/base"
export VCW_STACK_BASH_WRAPPER_ENVIRONMENT=
export VCW_STACK_BASH_WRAPPER_COMMAND="'sleep 0.5'"
export VCW_STACK_BASH_WRAPPER_PORTS=
export VCW_STACK_BASH_WRAPPER_VOLUMES=
export VCW_STACK_BASH_WRAPPER_NETWORKS=
export VCW_STACK_BASH_WRAPPER_DEPLOY_MODE="replicated"
export VCW_STACK_BASH_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export VCW_STACK_BASH_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END GLOBAL SETTINGS

# START TOPLEVEL SETTINGS
export VCW_STACK_GLOBAL_VOLUMES=
export VCW_STACK_GLOBAL_NETWORKS=
# END TOPLEVEL SETTINGS
