#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export VCWC_DC_GLOBAL_STACKS="image.wrapper"
export VCWC_DC_GLOBAL_STACKNAMES="VCWC_dc_image_wrapper"
export VCWC_DC_GLOBAL_BOOTSTACKS=""
export VCWC_DC_GLOBAL_RUNSTACKS="VCWC_dc_image_wrapper"
export VCWC_DC_GLOBAL_VOLUME_LIST=""
export VCWC_DC_GLOBAL='$VCWC_DC_GLOBAL_SERVICE_NAME,$VCWC_DC_GLOBAL_IMAGE_WRAPPER_NAME,$VCWC_DC_IMAGE_WRAPPER_ENVIRONMENT,$VCWC_DC_IMAGE_WRAPPER_COMMAND,$VCWC_DC_IMAGE_WRAPPER_VOLUMES,$VCWC_DC_IMAGE_WRAPPER_NETWORKS,$VCWC_DC_IMAGE_WRAPPER_PORTS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_MODE,$VCWC_DC_IMAGE_WRAPPER_LABELS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_LABELS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_PLACEMENT_CONSTRAINTS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_REPLICAS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_CPUS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_CPUS,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY,$VCWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY'
# END CONFIGURATION

# START GLOBAL SETTINGS
export VCWC_DC_GLOBAL_SERVICE_NAME="${VCWC_IMAGE}"
export VCWC_DC_GLOBAL_IMAGE_NAME="${VCWC_IMAGE}.${VCWC_FLAVOR}:${VCWC_TAG}"
export VCWC_DC_IMAGE_WRAPPER_ENVIRONMENT=
export VCWC_DC_IMAGE_WRAPPER_COMMAND="'sleep 0.5'"
export VCWC_DC_IMAGE_WRAPPER_PORTS=
export VCWC_DC_IMAGE_WRAPPER_VOLUMES=
export VCWC_DC_IMAGE_WRAPPER_NETWORKS=
export VCWC_DC_IMAGE_WRAPPER_DEPLOY_MODE="replicated"
export VCWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export VCWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END GLOBAL SETTINGS

# START TOPLEVEL SETTINGS
export VCWC_DC_GLOBAL_VOLUMES=
export VCWC_DC_GLOBAL_NETWORKS=
# END TOPLEVEL SETTINGS
