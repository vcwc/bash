#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export VWC_DC_GLOBAL_STACKS="image.wrapper"
export VWC_DC_GLOBAL_STACKNAMES="VWC_dc_image_wrapper"
export VWC_DC_GLOBAL_BOOTSTACKS=""
export VWC_DC_GLOBAL_RUNSTACKS="VWC_dc_image_wrapper"
export VWC_DC_GLOBAL_VOLUME_LIST=""
export VWC_DC_GLOBAL='$VWC_DC_GLOBAL_SERVICE_NAME,$VWC_DC_GLOBAL_IMAGE_WRAPPER_NAME,$VWC_DC_IMAGE_WRAPPER_ENVIRONMENT,$VWC_DC_IMAGE_WRAPPER_COMMAND,$VWC_DC_IMAGE_WRAPPER_VOLUMES,$VWC_DC_IMAGE_WRAPPER_NETWORKS,$VWC_DC_IMAGE_WRAPPER_PORTS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_MODE,$VWC_DC_IMAGE_WRAPPER_LABELS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_LABELS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_PLACEMENT_CONSTRAINTS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_REPLICAS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_CPUS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_CPUS,$VWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY,$VWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY'
# END CONFIGURATION

# START GLOBAL SETTINGS
export VWC_DC_GLOBAL_SERVICE_NAME="${VWC_IMAGE}"
export VWC_DC_GLOBAL_IMAGE_NAME="${VWC_IMAGE}.${VWC_FLAVOR}:${VWC_TAG}"
export VWC_DC_IMAGE_WRAPPER_ENVIRONMENT=
export VWC_DC_IMAGE_WRAPPER_COMMAND="'sleep 0.5'"
export VWC_DC_IMAGE_WRAPPER_PORTS=
export VWC_DC_IMAGE_WRAPPER_VOLUMES=
export VWC_DC_IMAGE_WRAPPER_NETWORKS=
export VWC_DC_IMAGE_WRAPPER_DEPLOY_MODE="replicated"
export VWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export VWC_DC_IMAGE_WRAPPER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="5m"
# END GLOBAL SETTINGS

# START TOPLEVEL SETTINGS
export VWC_DC_GLOBAL_VOLUMES=
export VWC_DC_GLOBAL_NETWORKS=
# END TOPLEVEL SETTINGS
