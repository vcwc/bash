#!/usr/bin/env bash

if [[ "$VCWC_EXECUTIONPLANE" == "$invocation" ]] ; then
    executionplane-message "ERROR" "${VCWC_EXECUTIONPLANE}: DIRECT RECURSE INTO SAME EXECUTION PLANE IS BLOCKED - ABORTING EXECUTION. MOST LIKELY CAUSE IS TARGETED API LAYER NOT PRESENT"
    exit;
fi
invocation="${VCWC_IMAGENAME}$@"
export VCWC_EXECUTIONPLANE=$( echo  "$invocation" | jq -aR . )

executionplane-message "INVOKED" ""
